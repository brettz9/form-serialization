{
  "name": "form-serialization",
  "version": "0.10.0",
  "description": "Serialize and deserialize HTML forms",
  "main": "dist/index.js",
  "browser": "dist/index.js",
  "module": "dist/index-es.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint --ext=js,html,md .",
    "rollup": "rollup -c",
    "start": "static -p 8057",
    "build-docs": "rm -rf docs/jsdoc/*;jsdoc --pedantic -c docs/jsdoc-config.js src",
    "open-docs": "open-cli http://localhost:8057/docs/jsdoc/ && npm start",
    "mocha": "mocha --ui mocha-qunit-ui --require @babel/register test/index.js",
    "test": "npm run eslint && npm run rollup && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/brettz9/form-serialization.git"
  },
  "keywords": [
    "form",
    "serialize"
  ],
  "author": "Brett Zamir",
  "contributors": [
    "Roman Shtylman <shtylman@gmail.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">= 7.0.0"
  },
  "homepage": "https://github.com/brettz9/form-serialization",
  "bugs": {
    "url": "https://github.com/brettz9/form-serialization/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@mysticatea/eslint-plugin": "^12.0.0",
    "browserslist": "^4.7.1",
    "eslint": "^6.5.1",
    "eslint-config-ash-nazg": "^11.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.12.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^12.1.0",
    "jsdoc": "^3.6.3",
    "jsdom": "^15.2.0",
    "mocha": "^6.2.2",
    "mocha-qunit-ui": "^0.1.3",
    "node-static": "^0.7.11",
    "open-cli": "^5.0.0",
    "rollup": "1.25.1",
    "rollup-plugin-babel": "^4.3.3",
    "typescript": "^3.6.4"
  }
}
